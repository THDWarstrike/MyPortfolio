<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyPortfolio.GITHUB"
             Title="GITHUB">
    <VerticalStackLayout BackgroundColor="Black">       
        
        <Label 
            Text="My GitHub"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
   
            <Label           
            Text="https://github.com/THDWarstrike?tab=repositories"
            VerticalOptions="Center"
            HorizontalOptions="Center"/>
 

        <StackLayout Orientation="Horizontal" HorizontalOptions="Center">
        <Button 
            Text="To Contact Page" 
            x:Name="BtncontactPage" 
            Clicked="BtncontactPage_Clicked"/>
        <Button 
            Text="Return to Previous Page" 
            x:Name="BtnPrevious" 
            Clicked="BtnPrevious_Clicked"/>
        </StackLayout>
    </VerticalStackLayout>
</ContentPage>